{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-len": [
            "error",
            120
        ],
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}
